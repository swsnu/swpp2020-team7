{
    "singleQuote": true,
    "parser": "typescript",
    "trailingComma": "all",
    "semi": true,
    "useTabs": true,
    "tabWidth": 4,
    "printWidth": 100,
    "endOfLine": "lf",
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "arrowParens": "always",
    "bracketSpacing": true
}